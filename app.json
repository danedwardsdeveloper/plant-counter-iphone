{
	"expo": {
		"name": "Plant-Counter",
		"slug": "plant-counter",
		"scheme": "myapp",
		"version": "1.3",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splashscreen.png",
			"resizeMode": "contain",
			"backgroundColor": "#3DA35D"
		},
		"ios": {
			"bundleIdentifier": "com.danedwardsdeveloper.plantcounter",
			"icon": "./assets/images/icon.png",
			"supportsTablet": true,
			"splash": {
				"image": "./assets/images/splashscreen.png",
				"resizeMode": "contain",
				"backgroundColor": "#3DA35D"
			}
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "acc273d6-fc7e-43a4-a060-3eee95fe96b6"
			}
		}
	}
}
